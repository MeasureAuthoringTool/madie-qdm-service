  <xs:complexType name="includeLibrarysType">
    <xs:sequence>
      <xs:element type="includeLibraryType" name="includeLibrary"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="cqlLookUpType">
    <xs:sequence>
      <xs:element type="xs:string" name="library"/>
