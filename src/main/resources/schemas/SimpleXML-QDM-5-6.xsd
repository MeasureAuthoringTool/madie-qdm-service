  <xs:complexType name="periodType">
    <xs:sequence>
      <xs:element type="xs:string" name="startDate"/>
      <xs:element type="xs:string" name="stopDate"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="calenderYear"/>
